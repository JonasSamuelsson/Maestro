<Project Sdk="Microsoft.NET.Sdk">

   <PropertyGroup>
      <TargetFramework>netstandard2.0</TargetFramework>
      <AssemblyName>Maestro</AssemblyName>
      <RootNamespace>Maestro</RootNamespace>
      <Version>3.5.0</Version>
      <Company />
      <Description>Maestro is a dependency injection / inversion of control library.</Description>
      <Authors>Jonas Samuelsson</Authors>
      <PackageLicenseUrl>https://github.com/JonasSamuelsson/Maestro/blob/master/LICENSE</PackageLicenseUrl>
      <PackageProjectUrl>https://github.com/JonasSamuelsson/Maestro</PackageProjectUrl>
      <RepositoryUrl>https://github.com/JonasSamuelsson/Maestro</RepositoryUrl>
      <PackageTags>maestro container ioc di</PackageTags>
      <LangVersion>latest</LangVersion>
   </PropertyGroup>

   <ItemGroup>
     <PackageReference Include="System.Reflection.Emit.Lightweight" Version="4.3.0" />
   </ItemGroup>

</Project>